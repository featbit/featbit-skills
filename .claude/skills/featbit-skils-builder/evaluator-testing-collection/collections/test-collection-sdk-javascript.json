    {
      "id": "js-sdk-001",
      "name": "js-sdk-basic-browser-setup",
      "skill": "featbit-javascript-client-sdk",
      "description": "Validates AI can guide developers through basic JavaScript SDK setup in browser environment with proper initialization.",
      "complexity_level": "basic",
      "task_type": "text-comparison",
      "input_task_prompt": "I'm building a web application and need to integrate FeatBit feature flags in the browser. How do I install and set up the JavaScript SDK? Show me the complete initialization code.",
      "evaluation_prompt": {
        "expected_result": {
          "must_include": [
            "npm install @featbit/js-client-sdk",
            "Import FbClientBuilder and UserBuilder",
            "FbClientBuilder() usage",
            "sdkKey() configuration",
            "streamingUri() configuration",
            "eventsUri() configuration",
            "user() configuration with UserBuilder",
            "build() method call",
            "waitForInitialization() async call"
          ],
          "should_include": [
            "Try-catch for initialization errors",
            "UserBuilder with custom properties",
            "Variation method example (boolVariation)",
            "WebSocket streaming explanation"
          ],
          "must_not_include": [
            "Node.js server SDK references",
            "@featbit/node-server-sdk package",
            "Server-side only patterns"
          ]
        },
        "evaluation_method": {
          "step_1": "Check package name is @featbit/js-client-sdk",
          "step_2": "Verify FbClientBuilder pattern is shown",
          "step_3": "Confirm all required configuration methods present",
          "step_4": "Check async initialization with waitForInitialization",
          "step_5": "Validate JavaScript syntax correctness"
        },
        "pass_criteria": [
          "Correct client SDK package mentioned",
          "All 9 'must_include' items present",
          "Proper builder pattern demonstrated",
          "Async/await or Promise handling shown",
          "At least 2 'should_include' items present",
          "No server-side SDK confusion"
        ],
        "fail_criteria": [
          "Wrong package (Node.js SDK)",
          "Missing required configuration",
          "No async initialization",
          "Invalid JavaScript syntax",
          "Server-side patterns suggested"
        ]
      },
      "prerequisites": [
        "Browser-based JavaScript project",
        "npm or package manager available",
        "FeatBit account with SDK key"
      ],
      "expected_duration": "4-5 minutes",
      "tags": ["sdk", "javascript", "browser", "client-side", "setup", "basic"]
    },
